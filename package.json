{
  "name": "v3-cmp-template",
  "private": false,
  "version": "0.0.11",
  "workspaces": [
    "packages/*",
    "site"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "main": "es/hdc.es.js",
  "typings": "typings/index.d.ts",
  "license": "MIT",
  "author": "protagonisths",
  "files": [
    "dist",
    "es",
    "packages",
    "typings"
  ],
  "scripts": {
    "site": "vite --config ./build/site.config.ts",
    "build": "vue-tsc --noEmit && vite build",
    "build:packages": "vue-tsc --noEmit && vite build --config ./build/packages.config.ts",
    "preview": "vite preview",
    "format": "pnpm prettier && pnpm lint",
    "lint": "eslint packages site --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write ."
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.2",
    "@types/prismjs": "^1.16.6",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1",
    "prismjs": "^1.25.0",
    "sass": "^1.45.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-md": "^0.11.6",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.8"
  }
}
